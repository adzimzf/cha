{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Chatbox CE",
  "version": "0.1.0",
  "identifier": "com.github.com/adzimzf/chatbox",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:1212",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Chatbox CE",
        "width": 1000,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [

      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon-1024.png",
      "icons/icon.svg",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
  }
}
